{
    "name": "samsonasik/expressive-redirect-handler",
    "type": "library",
    "description": "Expressive middleware for URL Redirect handling",
    "keywords": ["redirect","expressive"],
    "homepage": "https://github.com/samsonasik/ExpressiveRedirectHandler",
    "license": "MIT",
    "authors": [
        {
            "name": "Abdul Malik Ikhsan",
            "email": "samsonasik@gmail.com",
            "homepage": "http://samsonasik.wordpress.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^5.5|^7.0"
    },
    "require-dev": {
        "zendframework/zend-expressive": "^1.0",
        "phpunit/phpunit": "~4.0|~5.0",
        "satooshi/php-coveralls": "^1.0",
        "phpunit/phpcov": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "ExpressiveRedirectHandler\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ExpressiveRedirectHandlerTest\\": "test/"
        }
    },
    "config": {
        "process-timeout": 5000
    },
    "extra": {
        "zf": {
            "config-provider": "ExpressiveRedirectHandler\\ConfigProvider"
        }
    }
}
